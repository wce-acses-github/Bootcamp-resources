#include<bits/stdc++.h>
using namespace std;

int main(){

//Let's initialize an Array

int n; 
cin>>n;
  
int arr[n];

//Now we will be taking input for our Array from the user
for(int i = 0 ;i<n;i++){
    cin>>arr[i];
}

//Let's output the Array
for(int i=0 ;i<n;i++){
    cout<<arr[i];
}

    return 0;
}
